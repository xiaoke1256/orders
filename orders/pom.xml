  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
            <source>1.8</source>
            <target>1.8</target>
        </configuration>
       
      </plugin>
      
      <plugin>
       <groupId>org.apache.maven.plugins</groupId>
       <artifactId>maven-surefire-plugin</artifactId>
       <version>2.20</version>
       <configuration>
       	  <skip>true</skip>    
       	  <!-- 
        		排除掉特定的测试类 
         <excludes>
           <exclude>**/InitProduct.java</exclude>
         </excludes>
          -->
       </configuration>
     </plugin>
      
    </plugins>
  </build>
  
  
